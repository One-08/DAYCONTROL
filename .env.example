# Environment Configuration Template
# Copy this to a .env file and fill in your actual values

# Google Sign-In Configuration
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=YOUR_GOOGLE_IOS_CLIENT_ID.apps.googleusercontent.com
GOOGLE_ANDROID_CLIENT_ID=YOUR_GOOGLE_ANDROID_CLIENT_ID.apps.googleusercontent.com
GOOGLE_WEB_CLIENT_ID=YOUR_GOOGLE_WEB_CLIENT_ID.apps.googleusercontent.com

# Apple Sign-In Configuration
APPLE_TEAM_ID=YOUR_APPLE_TEAM_ID
APPLE_BUNDLE_ID=com.daycontrol.app

# App Configuration
APP_ENV=development
ENABLE_NOTIFICATIONS=true
MAX_DAILY_TASKS=5

# Database Configuration (usually defaults)
DB_NAME=daycontrol.db
